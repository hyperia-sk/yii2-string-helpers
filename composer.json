{
  "name": "hyperia/yii2-string-helpers",
  "description": "String helpers for your Yii2 app",
  "keywords": ["yii2", "helpers", "extension"],
  "type": "yii2-extension",
  "support": {
    "source": "https://github.com/hyperia-sk/yii2-string-helpers/"
  },
  "authors": [
    {
      "name": "Lukas Hrdlicka",
      "email": "lukas.hrdlicka@hyperia.sk"
    },
    {
      "name": "Lukas Mestan",
      "email": "lukas.mestan@hyperia.sk"
    }
  ],
  "require": {
    "yiisoft/yii2": "*"
  },
  "require-dev": {
    "symfony/var-dumper": "*",
    "phpunit/phpunit": "^5.7"
  },
  "autoload": {
    "psr-4": {
      "hyperia\\helpers\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "hyperia\\helpers\\tests\\": "tests/"
    }
  }
}